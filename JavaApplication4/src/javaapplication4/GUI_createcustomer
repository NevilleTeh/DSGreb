    private void btnSaveActionPerformed(java.awt.event.ActionEvent evt) {                                        
        String customerName = tfCustomerName.getText();
        int EAThour = (int)spinnerEAThour.getValue();
        int EATminute = (int)spinnerEATminute.getValue();
        int capacity = (int)cbCapacity.getSelectedItem();
        int SPlatitude = Integer.parseInt(tfSPlatitude.getText());
        int SPlongtitude = Integer.parseInt(tfSPlongtitude.getText());
        int destinationLatitude = Integer.parseInt(tfDestinationLatitude.getText());
        int destinationLongtitude = Integer.parseInt(tfDestinationLongtitude.getText());
        boolean premiumService = rbYes.isSelected();
        boolean notPremiumService = rbNo.isSelected();
        

        
    }                                       

