    private void btnSaveActionPerformed(java.awt.event.ActionEvent evt) {                                        
        String driverName = tfDriverName.getText();
        int capacity = (int)cbCapacity.getSelectedItem();
        int locationLatitude = Integer.parseInt(tfLatitude.getText());
        int locationLongtitude = Integer.parseInt(tfLongtitude.getText());
    }                                       

